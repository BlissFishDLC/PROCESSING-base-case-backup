void draw(){
  eye(300,300);
  eye(500,300);
}

void eye(int x, int y){
  float a; 
  a = atan2(mouseY - y, mouseX-x);
  fill(255,255,255);
  ellipse(x, y,100,60);
  fill(0,0,0);
  ellipse(x + 6*cos(a), y + 6*sin(a),50,50);
}
